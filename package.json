{
  "name": "vue-mixin-decorator-updated",
  "version": "1.3.2",
  "description": "Mixin decorators for Vue Component",
  "main": "dist/vue-mixin-decorator.umd.js",
  "typings": "./lib/index.d.ts",
  "keywords": [
    "vue",
    "typescript",
    "decorator"
  ],
  "author": "s00d",
  "license": "MIT",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:ts": "./node_modules/.bin/tsc -p src/tsconfig.json",
    "build:umd": "rollup -c",
    "test": "jest --config=jest.config.js --notify --coverage",
    "commit": "sgc",
    "semantic-release": "semantic-release -e ./release.config.js",
    "travis-deploy-once": "travis-deploy-once",
    "clean-dist": "del dist && del lib",
    "check-git": "git diff-index --quiet HEAD --",
    "update-dev": "git checkout dev && git pull",
    "merge-to-master": "git checkout master && git pull && git merge dev",
    "master-to-dev": "git checkout dev && git merge master",
    "prerelease": "yarn run check-git && yarn run clean-dist && yarn run update-dev && yarn test && yarn run build",
    "release": "yarn run merge-to-master && git push",
    "postrelease": "yarn run master-to-dev && git push"
  },
  "babel": {
    "presets": []
  },
  "files": [
    "lib",
    "dist"
  ],
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@vue/test-utils": "^1.1.3",
    "browser-env": "^3.3.0",
    "conventional-changelog-eslint": "^3.0.9",
    "del-cli": "^3.0.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "rollup": "^2.42.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "semantic-git-commit-cli": "^3.7.0",
    "semantic-release": "^17.4.2",
    "travis-deploy-once": "^5.0.0",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "typescript": "^3.5.3",
    "vue": "^2.5.13",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "vue-class-component": "^7.2.6"
  },
  "peerDependencies": {
    "typescript": ">= 2",
    "vue": "^2.5.0"
  },
  "repository": "git+https://github.com/s00d/vue-mixin-decorator.git",
  "bugs": {
    "url": "https://github.com/s00d/vue-mixin-decorator/issues"
  },
  "homepage": "https://github.com/s00d/vue-mixin-decorator#readme",
  "private": false
}
